<div class="container">
  <div class="row">
    <% for i in 0..2 %>
      <% if @images[i] %>
        <%= react_component('Photo', {title: @images[i][:title], image_url: @images[i][:uri]}) %>
      <% end %>
    <% end %>
  </div>
  <div class="row">
    <% for i in 3..5 %>
      <% if @images[i] %>
        <%= react_component('Photo', {title: @images[i][:title], image_url: @images[i][:uri]}) %>
      <% end %>
    <% end %>
  </div>
  <div class="row">
    <% for i in 6..8 %>
      <% if @images[i] %>
        <%= react_component('Photo', {title: @images[i][:title], image_url: @images[i][:uri]}) %>
      <% end %>
    <% end %>
  </div>
  <%= render 'pagination' %>
</div>